{
  "manifest_version": 3,
  "name": "Bark Twain",
  "description": "Unleash your inner corgi lover with this Chrome game! Pamper your virtual pup with treats, pets, and toys!ˁ(⚆ᴥ⚆)ˀ",
  "version": "1.0",
  "icons": {
    "16": "/assets/kiss.png",
    "48": "/assets/kiss.png",
    "128": "/assets/kiss.png"
  },
  "action": {
    "default_popup": "pop-up.html",
    "default_icon": "/assets/kiss.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  }
}